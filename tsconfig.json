{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "typeRoots": [
            "node_modules/@types",
            "src/@types"
        ],
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "temp"
    ]
}
